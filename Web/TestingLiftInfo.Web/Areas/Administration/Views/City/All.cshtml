@model TestingLiftInfo.Web.ViewModels.Administration.Cities.GetAllCityViewModel

@{
    this.ViewData["Title"] = "Градове";
}

<h2>@this.ViewData["Title"]</h2>
<div>
    <a class="btn btn-danger" asp-area="Administration" asp-controller="City" asp-action="Create">Добави</a>
</div>
@foreach (var setting in this.Model.Cities)
{
    <div>@*@setting.Id:*@ @setting.Name</div>
}
